<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.companyname.dms.settings.mapper.SystemAccountMapper">

    <select id="login" resultType="SystemAccount">
        select * from tb_system_account where username=#{username} and password=#{password}
    </select>
</mapper>
